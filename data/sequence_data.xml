<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Séquence pour les concessionnaires -->
        <record id="seq_hurimoney_concessionnaire" model="ir.sequence">
            <field name="name">Concessionnaire HuriMoney</field>
            <field name="code">hurimoney.concessionnaire</field>
            <field name="prefix">CONC/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Séquence pour les transactions -->
        <record id="seq_hurimoney_transaction" model="ir.sequence">
            <field name="name">Transaction HuriMoney</field>
            <field name="code">hurimoney.transaction</field>
            <field name="prefix">TRANS/%(year)s/%(month)s/</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>